<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Views="clr-namespace:WarehouseSystem.Views"
                    xmlns:ViewModels="clr-namespace:WarehouseSystem.ViewModels"
                    xmlns:ViewModelsMobile="clr-namespace:WarehouseSystem.ViewModels.Mobile"
                    xmlns:ViewsMobile="clr-namespace:WarehouseSystem.Views.Mobile">
    <DataTemplate DataType="{x:Type ViewModels:MainViewModel}">
        <Views:MainWindow />
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:ViewModel_StartScreen}">
        <Views:View_StartScreen />
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModelsMobile:MainViewModelMobile}" >
        <ViewsMobile:MobileMain />
    </DataTemplate>
</ResourceDictionary>